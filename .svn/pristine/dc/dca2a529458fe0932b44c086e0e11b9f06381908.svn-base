#test {
	padding: 0 20px 0 20px;

	white-space: nowrap;
	text-align: center;
	line-height: 80px;
	height: auto;
	margin-right: 100px;
	background-color: rgba(255, 255, 255, 0.54);
	border-color: rgb(255, 255, 255);
	border-width: 1px;
	border-style: solid;
	color: rgb(255, 255, 255);
	.border-radius: 5px;
	font-size: 28px;
	cursor: default;
}

#history {
	padding: 0 20px 0 20px;
	white-space: nowrap;
	text-align: center;
	line-height: 80px;
	height: auto;
	margin-left: 100px;
	background-color: rgba(255, 255, 255, 0.54);
	border-color: rgb(255, 255, 255);
	border-width: 1px;
	border-style: solid;
	color: rgb(255, 255, 255);
	border-radius: 5px;
	font-size: 28px;
	cursor: default;
}

#content {
	width: 100%;
	height: 100%;
	display: flex;
	justify-content: center;
	align-items: center;
	position: relative;
}

#bg {
	position: fixed;
	z-index: -1;
	width: 100%;
	height: 100%;
}

.dialog {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	z-index: 99;
	background-color: gray;
	opacity: 0.7;
}

.dialog-content {
	display: none;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	height: 400px;
	width: 600px;
	z-index: 100;
}

.dialog-scaleMsg {
	display: none;
	width: 100%;
	height: 100%;
	background-color: white;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: center;
}

.dialog-content-title {
	width: 100%;
	height: auto;
	text-align: center;
	height: 60px;
	line-height: 60px;
	background-color: #4CD964;
}

.line-thin {
	margin: 1px 0 1px 0;
	border-top: solid darkgray 1px;
	width: 100%;
}

.line-thick {
	margin: 1px 0 1px 0;
	border-top: solid darkgray 2px;
	width: 100%;
}

.scale-list {
	width: 100%;
	height: 100%;
	overflow: scroll;
	background-color: #5F9EA0;
	text-align: center;
}

.scale-list li {
	margin-top: 10px;
}

.dialog-close {
	font-size: 14px;
	width: 40px;
	height: 30px;
	background-color: #8F8F94;
	margin-bottom: 10px;
	margin-top: 5px;
	text-align: center;
	line-height: 30px;
}


.home {
	width: 40px;
	height: 40px;
	position: absolute;
	z-index: 100;
	top: 20px;
	right: 80px;
	line-height: 40px;
	text-align: center;
	color: rgb(214, 9, 9);
	font-family: SourceHanSansSC;
}

.on {
	display: initial;
	visibility: visible;
}

.off {
	display: none;
}

.on-flex {
	display: flex;
}

.menue-obj {
	width: calc(100% - 40px);
	height: calc(100% - 40px);
	position: absolute;
	background-color: white;
	border-radius: 5px;
	border: 1px solid darkgray;
}

